/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=abar);
    Not(in=b,out=bbar);
    And(a=abar,b=b,out=x);
    And(a=bbar,b=a,out=y);
    Or(a=x,b=y,out=out);
}